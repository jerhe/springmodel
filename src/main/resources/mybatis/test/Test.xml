<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jerhe.model.mybatis.Test">

	<select id="getAllTest" parameterType="map" resultType="map">
		SELECT
		aa.id "id",
		aa.title "title",
		aa.content "content"
		FROM
			test aa
	</select>

</mapper>